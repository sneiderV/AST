<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.sneider.imagebutton.MainActivity">

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="359dp"
        android:layout_height="247dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="8dp"
        android:contentDescription="@string/cont_js"
        android:onClick="mensajeJs"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/js" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="359dp"
        android:layout_height="248dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="5dp"
        android:contentDescription="@string/cont_react"
        android:onClick="mensajeReact"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton"
        app:srcCompat="@mipmap/react" />
</android.support.constraint.ConstraintLayout>
